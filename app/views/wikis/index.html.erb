<h1 class="text-center">My wikis</h1>
<% if current_user.standard? %>
  <%= render partial: 'table', locals: { free: true } %>
<% else %>
  <ul class="nav nav-tabs nav-justified">
    <li class="<%= active_view(false) %>"><%= link_to 'Public', wikis_view_path, method: :put, params: { paid: false } %></li>
    <li class="<%= active_view(true) %>"><%= link_to 'Private', wikis_view_path, method: :put, params: { paid: true } %></li>
  </ul>
  <%= render partial: 'table', locals: { paid: @view } %>
<% end %>
<div class="text-center">
  <%= link_to 'New wiki', new_wiki_path, class: "btn btn-default" %>
</div>
