<div class="table-responsive">
  <table class="table table-striped">
    <% @wikis.each do |wiki| %>
      <% if wiki.private == paid %>
        <tr>
          <td><%= link_to wiki.title, wiki %></td>
          <td><%= link_to 'edit', edit_wiki_path(wiki) %></td>
          <td><%= link_to 'delete', wiki_path(wiki), method: :delete %></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>
